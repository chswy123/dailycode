body{
    background-color: #efefef;
}
#guide{
    /*display:none; */
    position: absolute;
    z-index: 1000;
    cursor: pointer;
    width:100%;
    opacity: 0.8;
    background-color:#000;
    top:0;
}
.hide{
    display: none;
}

/*注册*/
.register-wrapper{
    z-index: 1001;
    position: relative;
    padding: 20px;
    text-align: center;
}
.scan-code{
    background-color: #ffffff;
    padding:35px 0;
    margin-top: 30%;
    border-radius: 5px;
}
.scan-code img{
    width: 225px;
    height: 225px;
    display:initial;
} 
header h3{
    color: #ff8903;
    margin: 20px 0;
}

.register{
    margin: 20px 0;
    position: relative;
    width: 100%;
}
.frm-register table{
    width: 100%;
}
table tr td{
    background: url("../img/icons.png") no-repeat;
    background-size: 175px 331px;
    padding-left: 40px;
    background-color: #ffffff;
    border-radius: 5px;
    display: inline-block;
    margin: 5px 0;
    width: 80%;
}
table tr td input{
    width: 100%;
    padding: 15px 0;
    background-color: #ffffff;
    border-radius: 5px;
}
table tr .name{
    background-position: 10px 13px;
}

table tr .phone{
    background-position: 10px -37px;
}
table tr .code{
    background-position: 10px 50px;
    position: relative;
}
table tr .code span{
    position: absolute;
    display: inline-block;
    padding: 8px 15px;
    top: 6px;
    right: 0;
    color: #f69834;
    border-left: 1px solid #dddddd;
    z-index: 9;
}
table tr .birthday{
    background-position: 10px -150px;
}
table tr .radio_txt{
    background: none;
    text-align: left;
    padding-left: 0;
}
.radiobox{
    padding: 15px 0;
}
.radiobox label{
    color: #ffffff;
    display: inline-block;
    margin-right: 15px;
}
.radiobox .em_default{
    display: inline-block;
    background-color: transparent;
    border: 1px solid #f19534;
    border-radius: 100px;
    width: 20px;
    height: 20px;
    margin: -4px 5px;
    position: relative;
}
.radiobox .em_hover{
    display: inline-block;
    background-color: #ffffff;
    border: 1px solid #f19534;
    border-radius: 100px;
    width: 20px;
    height: 20px;
    margin: -4px 5px;
}
.radiobox .em_default b.b_hover{
    display: inline-block;
    position: absolute;
    width: 10px;
    height: 10px;
    background: #f69834;
    border-radius: 5px;
    top: 5px;
    left: 5px;
}

.error-warning{
    background-color: #000000;
    opacity: 0.6;
    border-radius: 5px;
    padding: 40px 3%;
    position: absolute;
    color: #ffffff;
    width: 80%;
    left: 7%;
    top: 32%;
    z-index: 9;
}


footer .btn_ok{
    width:90%;
    margin: 0 auto;
    background-color: #ff8903;
    border-radius: 5px;
    padding: 15px 0;
    border: none;
    font-size: 1.2em;
    color: #ffffff;
}

/*个人中心*/
.info-wrapper{
    margin: 0;
}
.info-top{
    background: url("../img/top.png") no-repeat;
    background-size: 100%;
    text-align: center;
    margin: 0 auto;
    padding: 10px 0;
    position:relative;
}
.info-record{
    text-align: center;
    margin: 0 auto;
    padding: 15px 0;
    position:relative;
}
.info-record img{
    width:100%;
    height:auto;
    position:relative;
}
.info-record .record-title{
    position: absolute;
    top: 22%;
    width:100%;
    color: #fff;
    line-height: 25px;
}
.info-record .record-title b{
    font-size: 1.3em;
    font-weight: normal;
}
.record-container{
    padding:0 15px;
    position: relative;
}
.record-container h3{
    position: absolute;
    text-align: center;
    width:30%;
    left:35%;
    background-color: #ffffff;
    color:#ccc;
    top:-10px;
    z-index: 9;
}
.record-detail:nth-child(2){
    border-top: 1px solid #dddddd;
}
.record-detail{
    background:none;
    border-bottom: 1px solid #dddddd;
    padding: 15px 0;
    color: #666;
    display: block;
    position: relative;
    line-height: 25px;
}
.record-detail b{
    color:#b7b7b7;
    font-weight: normal;
}
.record-detail span{
    background:none;
    display: block;
    padding: 28px 0;
    position: absolute;
    top: 0;
    right: 0;
    color:#ff8903;
}
.info-top .info-gold{
    background: rgba(0,0,0,0.2); 
    top:20px;
    right:20px;
    display:block;
    position:absolute; 
    padding: 5px 8px;
    border-radius: 5px;
    color: #fff;
}
.info-gold i{
    display:inline-block;
    background:url('../img/icons.png') no-repeat;
    background-size: 135px;
    background-position: -99px -221px;
    color: transparent;
    width: 20px;
}
.info-top p{
    color:#ffffff; 
    line-height: 20px;
}
.info-top p i{
    display:inline-block;
    background:url('../img/icons.png') no-repeat;
    background-size: 135px;
    background-position: -98px 1px;
    color: transparent;
}
.info-top .info-top-detail{
    display: inline-block;
}
.info-top-detail img{
    width: 90px;
    height: 90px;
    border: 2px solid #ffffff;
    border-radius: 100px;
}

.info-tab{
    
}
.info-tab a{
    display:block;
}
.info-tab span{
    background: url("../img/icons.png") no-repeat;
    background-size: 160px 320px;
    display: block;
    padding: 10px 0;
    position: absolute;
    top: 0;
    right: 0;
    background-position: -60px -220px;
    width: 30px;
}
.info-tab .info-tab-detail{
    border-bottom: 1px solid #dddddd;
    padding: 15px 0;
    color: #999998;
    background: url("../img/icons.png") no-repeat;
    background-size: 160px 318px;
    display: block;
    padding-left: 50px;
    position: relative;
}
.info-tab .scan{
    background-position: -55px 8px;
}
.info-tab .record{
    background-position: -55px -50px;
}
.info-tab .address{
    background-position: -55px -108px;
}
.info-tab .exchange{
    background-position: -55px -168px;
}



/*兑换礼品信息*/
.exchange-wrapper{
    background-color: #ffffff;
    padding: 20px 12px;
}
.exchange-container{
    position:relative;
}
.exchange-container .exchange-img{
    background: #fff;
    height: auto;
}
.exchange-info-orange .exchange-img img{
    width: 90px;
    height: 90px;
    border: 1px solid #E9E5D2;
}
.exchange-info-blue .exchange-img img{
    width: 90px;
    height: 90px;
    border:1px solid #80a6b5;
}
.exchange-container .exchange-txt{
    display:inline-block;
    width: 60%;
    line-height: 40px;
    padding: 10px 0;
    position:relative;
}
.exchange-txt h3{
    position:relative;
    width: 108px;
    height:20px;
    line-height:20px;
    text-overflow:ellipsis;
    white-space:nowrap;
    overflow:hidden;
    text-align: left;
}
.exchange-info-orange a{
    position:absolute;
    color:#bfad58;
    text-decoration: none;
    top:0;
    right:0;
}
.exchange-info-blue a{
    position:absolute;
    color:#ffffff;
    text-decoration: none;
    top:0;
    right:0;
}
.exchange-info-orange p{
    color:#bdad63;
    position: relative;
    padding-left: 20px;
    text-align: left;
}

.exchange-info-blue p{
    color:#ffffff;
    position: relative;
    padding-left: 20px;
    text-align: left;
}
.exchange-container p i{
    /* display: inline-block; */
    background: url('../img/icons.png') no-repeat;
    background-size: 170px;
    color: transparent;
    width: 18px;
    position: absolute;
    height: 16px;
    left: 0;
    top: 13px;
}
.exchange-info-orange p i{
    background-position: -35px -86px;
}
.exchange-info-blue p i{
    background-position: -35px -141px;
}
.exchange-container .shadow{
    width:100%;
}
.exchange-info-orange{
    background:url('../img/bg-orange.png') repeat;
    background-size: 100%;
    text-align: center;
    /*box-shadow: 0 0 10px 0;*/
    /*-webkit-box-shadow:0 0 10px rgba(0, 0, 0, .5);  
    -moz-box-shadow:0 0 10px rgba(0, 0, 0, .5);  
    box-shadow:0 0 10px rgba(0, 0, 0, .5);  */
    position:relative;
    padding: 15px;
}
.exchange-info-blue{
    background:url('../img/bg-blue.png') repeat;
    background-size: 100%;
    text-align: center;
    /* box-shadow: 0 0 10px 0; */
    position: relative;
    padding: 15px;
}


/*收货地址*/
.address-wrapper{
    padding-top: 50px;
    text-align: center;
}
.address-wrapper form table{
    padding-top: 50px;
    border-top: 1px solid #ddd;
}
.address-wrapper form table tr td{
    margin: 0 auto;
    border-bottom: 1px solid #ddd;
    border-radius: initial;
    width: 90%;
    padding-left: 10%;
}
.address-wrapper form table tr .name{
    background-position: 5px 13px;
}

.address-wrapper form table tr .phone{
    background-position: 5px -40px;
}
.address-wrapper form table tr .birthday{
    background-position: 5px -100px;
}
button.btn_ok{
    width:80%;
    background-color: #ff8903;
    border-radius: 5px;
    padding: 15px 0;
    border: none;
    font-size: 1.2em;
    color: #ffffff;
    margin-top: 30px;
}

/*商品扫一扫*/
.goods-wrapper{
    background-color: #ffffff;
    border-bottom: 1px solid #ddd;
    padding:25px 10px 10px 10px;
}
.goods-wrapper img{
    border: 1px solid #ddd;
    width: 115px;
    height: 100px;
}
.goods-txt{
    width: 55%;
    display:inline-block;
    margin-left: 16px;
    line-height: 25px;
}
.goods-txt p{
    color:#999999;
}
.goods-txt p:nth-child(1) label{
    text-align: left;
    display: inline-block;
    width: 112px;
    vertical-align: top;
}

.goods-desc,.goods-desc p{
    color:#999999;
    margin: 20px 0;
    line-height: 25px;
}
.goods-desc p b{
    color:#ff8903;
    font-size: 1.2em;
}
.goods-desc label{
    color:#666;
}

/*礼品商城*/
.mall-top-wrapper{
    position: relative;
}
.mall-top{
    background-color: #ffffff;
    padding: 10px;
    text-align: left;
}
.mall-top-wrapper input{
    width: 80%;
    padding: 13px 0;
    position: absolute;
    top: 0;
    padding-left: 20%;
    background: #ffffff url("../img/icon_search.png") no-repeat;
    background-size: 32px 132px;
    background-position: 20px -85px;
    border-radius: 0;
}
.mall-top span{
    padding: 3px;
    border-right: 1px solid #dddddd;
    text-align: center;
    width: 42%;
    display: inline-block;
    position: relative;
}
.mall-top span:first-child{
    width: 35%;
}
.mall-top span i{
    background: url("../img/icon_search.png") no-repeat;
    background-size: 30px 115px;
    background-position: 0 0;
    width: 10px;
    display: inline-block;
    height: 13px;
    position: absolute;
    right: 8%;
    top: 25%;
}
.mall-top a{
    background: url("../img/icon_search.png") no-repeat;
    background-size: 25px 106px;
    background-position: 4px -33px;
    width: 25px;
    display: inline-block;
    height: 25px;
    /* margin: 5px 7px; */
    position: absolute;
    right: 30px;
}

.mall-wrapper{
    /*margin: 20px 0;*/
    position: relative;
}
.mall-wrapper-search{
    width: 100%;
    background-color: #000000;
    opacity: 0.7;
    z-index: 999;
    position: absolute;
    top: -20px;
    margin: 0;
}
.mall-container{
    background-color: #ffffff;
    padding: 20px 15px;
    margin:20px 0;
}
.order-con{
    background-color: #ffffff;
    padding: 0 15px 15px;
    margin: 20px 0;
}
.order-con h2{
    padding:10px 0;
}
.mall-img img{
    width: 95px;
    height: auto;
    border:1px solid #ddd;
}
.mall-txt{
    display: inline-block;
    width: 62%;
    position: relative;
    /* float:right; */
    margin-left: 10px;
}
.mall-txt label{
    color: #cccccc;
}
.mall-txt label em{
    color: #ff4444;
}
.mall-txt p{
    color:#ff8903;
    position: relative;
    padding-left: 20px;
    text-align: left;
    /* line-height: 55px; */
    margin-top: 15px;
}
.mall-txt p i{
    /* display: inline-block; */
    background: url('../img/icons.png') no-repeat;
    background-size: 155px;
    color: transparent;
    width: 22px;
    position: absolute;
    height: 30px;
    left: 0;
    top: -5px;
}
.mall-txt p i{
    background-position: -115px -250px;
}
.btn_change{
    width: 80px;
    position: absolute;
    color: #ffffff;
    top: 45px;
    right: -10px;
    background-color: #ff8903;
    border-radius: 5px;
    padding: 7px 0;
    border: none;
    font-size: 1em;
}
.btn_change_default{
    width: 80px;
    position: absolute;
    color: #333333;
    top: 45px;
    right: -10px;
    background-color: #e9e9e9;
    border-radius: 5px;
    padding: 7px 0;
    border: none;
    font-size: 1em;
}

.detail-add button{
    position: relative;
    top: 15px;
    left: 16px;
}


/*订单中心*/
.order-wrapper{
    margin: 0 0 20px 0;
}
.order-wrapper a{
    display:block;
}
.order-address{
    background-color: #5f759e;
    padding:23px;
    position: relative;
    padding-left: 30px;
}
.order-address i:nth-child(1){
    position:absolute;
    display: inline-block;
    background: url('../img/icons.png') no-repeat;
    background-size: 135px;
    background-position: -98px -117px;
    color: transparent;
    width: 25px;
    left: 3px;
    height: 35px;
}
.order-address i:nth-child(2){
    position:absolute;
    display: inline-block;
    background: url('../img/icons.png') no-repeat;
    background-size: 139px;
    background-position: -20px -17px;
    color: transparent;
    width: 25px;
    right: 10px;
    height: 35px;
    top: 40px;
}
.order-address p,.order-address label{
    color:#ffffff;
    margin-right: 10px;
    line-height: 25px;
}
.order-address p:nth-child(4),.order-address p:nth-child(4) label{
    color:#b2c5e7;
}
.mall-btn{
    text-align: center;
    margin:0 auto;
}
.mall-btn button{
    width:40%;
}
.mall-btn button:nth-child(2){
    background-color: #cccccc;
    color:#000;
}

.order-num{
    position: relative;
    color: #888888;
}
.order-num .num{
    position: absolute;
    top: -5px;
    left: 45px;
    border-radius: 5px;
    background-color: #f6f6f6;
    border: 1px solid #dcdcdc;
    width: 115px;
}
.num i{
    font-size: 1.8em;
    font-style: normal;
    width: 27px;
    color: #b4b4b4;
    text-align: center;
}
.num .num_change{
    background-color: #fff;
    /*padding: 0 16px;*/
    border-left: 1px solid #dcdcdc;
    border-right: 1px solid #dcdcdc;
    font-size: inherit;
    height: 33px;
    display: inline-block;
    line-height: 33px;
    text-align: center;
    color:#333;
    width: 59px;
}
.goods-con button{
    margin-left: 10%;
}